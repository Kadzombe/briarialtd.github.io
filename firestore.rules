rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /contactSubmissions/{docId} {
      allow create: if true;
      allow read, update, delete: if false;
    }
    match /quoteSubmissions/{docId} {
      allow create: if true;
      allow read, update, delete: if false;
    }
     match /demoSubmissions/{docId} {
      allow create: if true;
      allow read, update, delete: if false;
    }
  }
}